{
  "name": "medusa-backend",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "medusa build",
    "build:all": "medusa build",
    "build:admin": "medusa build --admin-only",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "fix-admin": "medusa exec ./src/scripts/fix-admin-auth.ts",
    "db:migrate": "medusa db:migrate",
    "start": "medusa start",
    "start:prod": "NODE_ENV=production medusa start",
    "start:with-migrations": "medusa db:migrate && medusa start",
    "start:render": "medusa build && medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.8.4",
    "@medusajs/cli": "2.8.4",
    "@medusajs/file-s3": "^2.8.4",
    "@medusajs/framework": "2.8.4",
    "@medusajs/medusa": "2.8.4",
    "@medusajs/notification-sendgrid": "^2.8.4",
    "@medusajs/payment-stripe": "^2.8.4",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@supabase/supabase-js": "^2.50.1",
    "@types/jsonwebtoken": "^9.0.10",
    "awilix": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.2",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.8.4",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  }
}